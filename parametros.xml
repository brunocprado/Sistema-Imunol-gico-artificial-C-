<?xml version="1.0" encoding="UTF-8"?>

<!--
    * Parametros do sistema. <br>
    * Temporizações serão reajustadas por meio de alg genético.
    * @author Bruno Prado
-->

<parametros>
    <!-- Simulacao -->
<!--    <TAMX>1600</TAMX>-->
<!--    <TAMY>900</TAMY>-->
    <TAM_MEDIO_INFERIOR>400</TAM_MEDIO_INFERIOR>
    <TAM_MEDIO_SUPERIOR>1100</TAM_MEDIO_SUPERIOR>
    
    <!--
        Fonte :In the US this is usually expressed as 4,000–11,000 white blood cells per microliter of blood. 
        Total leukocytes: 4.00-11.0 x 10 9/L
        Adult	3.5 3.9 4.1 4.5	  9.0 10.0 10.9 11    - x109/L
    -->
    
    <!-- Temporizacao -->
    <TEMPO_PROCESSAMENTO>30</TEMPO_PROCESSAMENTO>
    <TEMPO_PROPAGACAO_QUIMICOS>80</TEMPO_PROPAGACAO_QUIMICOS>
    <TEMPO_FAGOCITACAO>300</TEMPO_FAGOCITACAO>
    <DELAY_PROPAGACAO>800</DELAY_PROPAGACAO>
    <DELAY_ANTI>500</DELAY_ANTI>
    
    <!-- Proporcoes -->
    <MACROFAGOS>0.053</MACROFAGOS>
    <NEUTROFILOS>0.65</NEUTROFILOS>
    <LINFOCITOS>0.26</LINFOCITOS>

    <!-- Tempo de vida 200 = 24 horas -->
    <VIDA_ANTIGENO>300</VIDA_ANTIGENO>
    <VIDA_MACROFAGO>400</VIDA_MACROFAGO>
    <VIDA_ANTICORPO>800</VIDA_ANTICORPO>
    <VIDA_CELULA_B>350</VIDA_MACROFAGO>


    <INCUBACAO_ANTIGENO>150</INCUBACAO_ANTIGENO>

    <!-- Chance de reprodução -->
    <CHANCE_MACROFAGO>0.2</CHANCE_MACROFAGO>

</parametros>
